<game_config>
<game_key>arma2oa_win32</game_key>
<protocol>lgsl</protocol>
<lgsl_query_name>arma2oa</lgsl_query_name>
<game_name>Arma 2 Operation Arrowhead</game_name>
<server_exec_name>arma2oaserver.exe</server_exec_name>
  <cli_template>%PORT% %PLAYERS%</cli_template>
  <cli_params>
    <cli_param id="PORT" cli_string="-netlog -maxMem=2047 -cpuCount=8 -port=" />
    <cli_param id="PLAYERS" cli_string="-maxplayers=" />
  </cli_params>
<console_log>profile/server_console.log</console_log>
<max_user_amount>64</max_user_amount>
<control_protocol>armabe</control_protocol>
<mods>
 <mod key='arma2oa'>
  <name>none</name>
  <installer_name>arma2oa</installer_name>
 </mod>
</mods>
  <server_params>
    <param key="-profiles=" type="select">
	  <option value="profile5 -cfg=profiles5/arma2.cfg -config=server5.cfg">Servidor Publico sin Mods</option>
      <option value="profile1 -cfg=profiles1/arma2.cfg -config=server1.cfg -mod=@ACE -mod=@CBA">Servidor Publico ACE</option>
      <option value="profile2 -cfg=profiles2/arma2.cfg -config=server2.cfg -mod=@ACE -mod=@ACRE -mod=@CBA">Servidor Publico ACE+ACRE</option>
	  <option value="profile7 -cfg=profiles7/arma2.cfg -config=server7.cfg -mod=@ACE -mod=@ACRE -mod=@CBA -mod=@CiaEagleIslas -mod=@CiaEagleMods -mod=@CiaEagleUnits">Servidor Publico Mods Oficiales</option>
	  <option value="profile6 -cfg=profiles6/arma2.cfg -config=server6.cfg">Servidor Privado sin Mods</option>
	  <option value="profile3 -cfg=profiles3/arma2.cfg -config=server3.cfg -mod=@ACE -mod=@CBA">Servidor Privado ACE</option>
      <option value="profile4 -cfg=profiles4/arma2.cfg -config=server4.cfg -mod=@ACE -mod=@ACRE -mod=@CBA">Servidor Privado ACE+ACRE</option>
	  <option value="profile8 -cfg=profiles8/arma2.cfg -config=server8.cfg -mod=@ACE -mod=@ACRE -mod=@CBA -mod=@CiaEagleIslas -mod=@CiaEagleMods -mod=@CiaEagleUnits">Servidor Privado Mods Oficiales</option>
      <caption>Seleccionar Lanzamiento</caption>
      <desc>Iniciar Servidor con el mod seleccionado</desc>
      <options>ns</options>
    </param>
  </server_params>
</game_config>