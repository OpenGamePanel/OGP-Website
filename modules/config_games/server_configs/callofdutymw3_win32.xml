<game_config>
  <game_key>cod_mw3_win32</game_key>
  <protocol>lgsl</protocol>
  <lgsl_query_name>modernwarfare3</lgsl_query_name>
  <installer>steamcmd</installer>
  <game_name>CoD: Modern Warfare 3</game_name>
  <server_exec_name>iw5mp_server.exe</server_exec_name>
  <query_port type="add">-1</query_port>
  <cli_template>+set dedicated 2  %IP% %PORT% %QUERY_PORT% %PLAYERS% +set sv_maprotation default +start_map_rotate</cli_template>
  <cli_params>
    <cli_param id="IP" cli_string="+set net_ip" options="s" />
    <cli_param id="PORT" cli_string="+set net_queryPort" options="s" />
	<cli_param id="QUERY_PORT" cli_string="+set net_masterServerPort" options="s" />
    <cli_param id="PLAYERS" cli_string="+set sv_maxclients" options="s" />
  </cli_params>
  <max_user_amount>18</max_user_amount>
  <mods>
    <mod key="modernwarfare3">
      <name>none</name>
	  <installer_name>42750</installer_name>
    </mod>
  </mods>
</game_config>
